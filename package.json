{
  "name": "webpack-frontend-core",
  "version": "1.0.0",
  "description": "webpack frontend core",
  "main": "./src/scripts/index.js",
  "scripts": {
    "start": "live-server --port=8008 --open=./dist --ignore=src,node_modules --watch=./dist",
    "build-prod": "webpack --progress -colors --watch",
    "build-dev": "rm -rf ./dist && mkdir ./dist && webpack --progress -colors --watch",
    "test": "babel-tape-runner ./test/**/*-test.js | tap-summary"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bendfold/webpack_frontend_core.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "es6",
    "stylus",
    "eslint",
    "tape"
  ],
  "author": "Owen Cowdry",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "live-server": "^1.1.0",
    "nib": "^1.1.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.2.0",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {}
}
