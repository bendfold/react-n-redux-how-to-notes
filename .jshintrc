// http://jshint.com/docs/options/ 
// Taken from https://raw.githubusercontent.com/airbnb/javascript/master/linters/.jshintrc#
{
	"browser": true,
	"jquery": true,
	"node": true,
	"esversion": 6,
	"camelcase": true,
	"eqeqeq": true,
	"indent": 2,
	"latedef": true,
	"maxlen": 100,
	"newcap": true,
	"quotmark": "single",
	"strict": true,
	"undef": true,
	"unused": true,
	"eqnull": true
}