{
  "name": "webpack-2-frontend-core",
  "version": "2.0.0",
  "description": "webpack 2 frontend core",
  "main": "./src/index.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development && webpack-dev-server -d",
    "prod": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "lint:js": "eslint app/ webpack.*.js --cache",
    "clean": "rimraf ./dist/*",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://ocowdry@bitbucket.org/ocowdry/webpack_core.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "es6",
    "stylus",
    "eslint"
  ],
  "author": "Owen Cowdry",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|styl)$": "<rootDir>/src/styles/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/legacy_notes_and_workbook/",
      "<rootDir>/client/src/stylesheets/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babili-webpack-plugin": "^0.1.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^22.4.0",
    "nib": "^1.1.2",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "pug": "^2.0.0-rc.1",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tap-summary": "^3.0.2",
    "tape": "^4.6.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "html-loader": "^0.5.5",
    "markdown-loader": "^2.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  }
}
